<MudNavMenu Color="Color.Primary">
    @foreach (string Category in Categories) {
        <MudNavLink Href="blog" Match="NavLinkMatch.Prefix" Disabled="true"><MudText Typo="Typo.button" Class="mud-text-disabled">@Category</MudText></MudNavLink>
        @foreach (MudBlocks.Models.Block b in Blocks.Blocks.Where(b => b.Category == Category)) {
            <MudNavLink Href="@b.Url" Match="NavLinkMatch.Prefix">
                <MudImage Src="@b.Image" Alt="@b.Name" Elevation="25"  Fluid="true" Class="rounded-lg"/>
            </MudNavLink>
        }
    }
</MudNavMenu>